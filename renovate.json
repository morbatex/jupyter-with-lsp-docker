{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
    "config:base"
  ],
  "packageRules": [
    {
      "matchDatasources": ["docker"],
      "matchPackageNames": ["jupyter/scipy-notebook"],
      "versioning": "regex:^((?<compatibility>.*)-)(?<major>\d+)(\.(?<minor>\d+))?(\.(?<patch>\d+))?$"
    }
  ]



}
